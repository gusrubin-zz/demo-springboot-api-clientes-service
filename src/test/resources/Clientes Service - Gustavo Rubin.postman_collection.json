								"exec": [
									"postman.clearGlobalVariable(\"cliente-id\");",
									"var cpf = postman.getGlobalVariable(\"cpf\");",
									"if (cpf != null) {",
									"    cpf = Number(cpf);",
									"    cpf++;",
									"    postman.setGlobalVariable(\"cpf\", cpf);",
									"} else {",
